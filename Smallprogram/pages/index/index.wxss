.list_li{
  float: left;
  width: 370rpx;
}
.list_li .list_li_title_left{
  font-size: 26rpx;
  float: left;
  color: #232323;
  font-family: "微软雅黑"；
}
.list_li .list_li_title_right{
  font-family: "微软雅黑";
  font-size:24rpx;
  float: right;
  color: #979797
}
.list_li image{
  width: 100%;
  max-height:100%;
}
.list_li:nth-child(2n){
  float: right
}
.list_li{
  margin-bottom: 10rpx
}
.clear{
  clear: both
}
.list_ul{
  margin-top: 10rpx;
}
.list_li_title{
  margin-bottom: 30rpx;
  height: 45rpx;
  overflow: hidden;
}
.list_li_title .list_li_title_left{
  margin-left: 10rpx;
}
.list_li_title .list_li_title_right{
  margin-right: 10rpx;
}

.footer{
  position: fixed;
  bottom: 0;
  background: #fff;
  width: 100%;
  border-top: 1px solid #b8b0b3;
  padding-top: 16rpx;
}
.footer .footer_img{
  float: left;
  width: 33.33%;
  text-align: center;
}
.footer .footer_img image{
  width: 43rpx;
  height:43rpx;
}
.footer .footer_img text{
  display: block;
  font-size: 18rpx;
  padding-bottom: 5rpx;
}
