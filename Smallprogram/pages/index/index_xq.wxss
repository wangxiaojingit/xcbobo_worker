page{
  position: relative;
  width: 100%;
  height: 100%;
  background: #1d1d1d;
  overflow: hidden;
}
.video{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 5;
  top: 94rpx;
}
.coverview{
  background: rgba(255, 255, 255, 0.3);
  width: 96%;
  left: 2%;
  border-radius:5px;
  position: absolute;
  z-index: 999;
  font-size: 28rpx;
  color: #e7a67c;
  bottom: 5%;
  word-break: break-all;
  word-wrap:break-word;
  white-space:pre-line;
}
.zhibo_iframe{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 20;

}
.zhibo_iframe_top{
  padding:10rpx;
}
.zhibo_iframe_top_left{
  width: 100%;
  background: rgba(50, 48, 41, 0.5);
  /* border-radius: 100rpx; */
  /* float: left; */
  /* margin-left: 10rpx; */
}
.headimg{
  width: 74rpx;
  height: 74rpx;
  border-radius: 50%;
  float:left;
  background-size: cover;
background-position:top center;
}
.clear{
  clear: both
}
.useriframe{
  float: left
}
.useriframe{
  width: 200rpx;
  margin-left: 15rpx;
}
.useriframe text{
  display: block;
  margin-top: 10rpx;
  color: #fff;
}
.useriframe .username{
  font-size: 19rpx;
  /* width: 89rpx; */
  white-space: nowrap;
  overflow: hidden;
  additive-symbols: ellipsis;  
}
.useriframe .gzrs{
  font-size: 18rpx;
  margin-top: 10rpx;
}
.zhibo_iframe_top_left button{
/*  width: 90rpx;*/
  height: 50rpx;
  background: #e7a67c;
  color: #fff;
  float: right;
  text-align: center;
  line-height: 50rpx;
  margin-right: 40rpx;
  font-size: 18rpx;
  border-radius: 80rpx;
  margin-top: 12rpx;
}
.zhibo_iframe_top_right{
  color: #fff;
  font-size: 18rpx;
  float: right;
  width: 422rpx;
  position: relative;
}
.zhibo_iframe_top_right .leitais{
  width: 72rpx;
  height: 72rpx;
  font-size: 12px;
  border-radius: 20rpx;
  line-height: 72rpx;
  text-align: center;
  float: left;
  display: block;
  color: #fff
}
.zhibo_iframe_top_right scroll-view{
  width: 340rpx;
  height: 72rpx;
  overflow: auto;
  float: left;
  white-space: nowrap;
  overflow:scroll;
}
.zhibouserlist{
  display: inline-block;
}
.zhibo_iframe_top_right scroll-view image{
  width: 57rpx;
  height: 57rpx;
  border-radius: 50%;
  margin-top: 10rpx;
  margin-left: 5rpx;
  border:2rpx solid #fff;
  display: inline-block;
}
.userid_iframe{
  float: left;
  background: rgba(50, 48, 41, 0.5);
  height: 30repx;
  font-size: 20rpx;
  color: #fff;
  margin-left: 10rpx;
  margin-top: 15rpx;
  border-radius: 20rpx;
}
.zhibo_iframe_top_bottom_right{
  float: right;
  font-size: 20rpx;
  background: rgba(50, 48, 41, 0.5);
  border-radius: 20rpx;
  margin-top: 15rpx;
  margin-right: 10rpx;
}
.zhibo_iframe_top_bottom_right text{
  display: block;
  color: #fff
}
.liaotiframe{
  position: absolute;
  bottom: 0;
  height: 410rpx;
}
.gonggaousername{
  color: #ffffc4;
  font-size: 32rpx;
}
.gonggaoneirong{
  font-size: 32rpx;
  color: #b7dbff
}
.pinglun_iframe{
  position: absolute;
  bottom: 0;
  z-index: 40;
  width: 720rpx;
}
.pinglun_iframe input{
  display: block;
  font-size: 24rpx;
  background: rgba(95, 87, 83,0.5);
  text-indent: 10px;
  color: #fff;
  width: 600rpx;
  height: 70rpx;
  border-radius: 30rpx;
  float: left;
  margin-left: 15rpx;
}
.fenxiang{
  width: 50rpx;
  height: 73rpx;
  float: right;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  padding: 0;
  margin-right: 10rpx;
  margin-top: 10rpx;
}

